{
    "name": "Messaging From Popup To Content",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": [
            "https://developer.chrome.com/extensions/manifest"
          ],
          "js": ["content.js"]
        }
    ],
    "browser_action":{
        "default_popup":"popup.html"
    }
}